{
  "arch": "arm",
  "cpu": "arm7tdmi",
  "data-layout": "e-m:e-p:32:32-i64:64-v128:64:128-a:0:32-n32-S64",
  "disable-redzone": true,
  "no-compiler-rt": true,
  "executables": true,
  "llvm-target": "thumbv4t-none-eabi",
  "morestack": false,
  "os": "none",
  "relocation-model": "static",
  "target-endian": "little",
  "target-word-size": "32",
  "target-pointer-width": "32",
  "min-atomic-width": 0,
  "max-atomic-width": 0,
  "linker": "armv4t-none-eabi-ld",
  "linker-flavor": "ld",
  "linker-is-gnu": true,
  "ar" : "armv4t-none-eabi-ar",
  "post-link-args" : {
    "ld": ["-T", "gba.ld", "-u", "_init", "-u", "_header"]
  }
}


